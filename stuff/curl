curl -X POST "http://127.0.0.1:5000/api/concerts" -d '{"artist":"peter","location":"somewhere","date":"2021-10-29T18:00:00.000Z","link":"http://some.link"}' -H 'Content-Type: application/json'
curl -X POST "http://127.0.0.1:5000/api/concerts" -d '{"artist":"peter","location":"somewhere","date":"2021-10-29T18:00:00.000Z","link":"not a link"}' -H 'Content-Type: application/json'